{
  "presets": [
    ["@babel/preset-env", {
    "targets": [
      "last 2 versions",
      "not dead",
      "> 0.2%",
      "not ie 11"
    ],
    "useBuiltIns": "entry",
    "corejs": 3            
  }],
  "@babel/preset-react"
  ],
  "plugins": [
    "react-hot-loader/babel",
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-syntax-dynamic-import"
  ]
}